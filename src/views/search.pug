extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching for: #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 검색결과가 없습니다.
        each video in videos
            +videoBlock({
                title:video.title,
                views:video.views,
                videoFile:`/${video.fileUrl}`,
                id:video.id
            })